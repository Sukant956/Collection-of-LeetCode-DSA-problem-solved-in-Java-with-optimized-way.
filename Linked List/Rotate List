// Problem: Rotate List
// Platform: LeetCode and Striver
// Approach: Circular Linked List
// Time Complexity: O(N)
// Space Complexity: O(1)

// Code 

class Solution {
    public ListNode rotateRight(ListNode head, int k) {
        if (head == null || k == 0)
        {
            return head;
        }
        ListNode temp = head;
        int n = 0;
        while(temp.next != null)
        {
            temp = temp.next;
            n++;
        }
        temp.next = head;
        k = k %(n +1);
        int jump = n-k;
        temp = head;
        while(jump > 0)
        {
            temp = temp.next;
            jump --;
        }
        ListNode returnhead = temp.next;
        temp.next = null;
        return returnhead;
    }
}
