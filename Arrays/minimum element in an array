// Problem: minimum element in an array
// Platform: leetcode

// Code

class minimum 
{
  public static void main(String []args){
    int arr = {1,2,3,4};
    int minimum = arr[0];

    for(int i = 0;i<arr.length;i++){
     if(arr[i] < minimum){
       minimum = arr[i];
     }
    }
    System.out.println(minimum);
  }
}
