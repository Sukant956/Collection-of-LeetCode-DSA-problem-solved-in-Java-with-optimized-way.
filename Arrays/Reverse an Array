// Problem: Reverse an Array


class Test {
  public static void main(String st[]){
    int arr[] = {2,4,5,6,7};

    int i = 0;
    int j = arr.length - 1;
    int temp;

  while(i < j){
    temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
    i++;
    j--;
  }
System.out.println("Reversed Array");
  }
}
