// Problem: Find the Duplicate Number
// Platform: Leetcode and Striver
// Approach: Floyd's Cycle Detection
// Time Complexity:O(n)
// Space Complexity:O(1)

// Code

class Solution {
    public int findDuplicate(int[] nums) {
        int slow = 0;
        int fast = 0;

        do{
            slow = nums[slow];
            fast = nums[nums[fast]];
        }
        while(fast != slow);
        slow = 0;
        while(fast != slow){
            slow = nums[slow];
            fast = nums[fast];
        }
        return slow;
    }
}
